{
  "name": "tree-sitter-fasterbasic",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for FasterBASIC",
  "main": "bindings/node",
  "keywords": [
    "tree-sitter",
    "parser",
    "fasterbasic",
    "basic"
  ],
  "author": "FasterBASIC Team",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.22.0"
  },
  "scripts": {
    "build": "tree-sitter generate && node-gyp build",
    "test": "tree-sitter test",
    "parse": "tree-sitter parse"
  },
  "tree-sitter": [
    {
      "scope": "source.fasterbasic",
      "file-types": [
        "bas",
        "bi",
        "bm"
      ],
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm"
    }
  ]
}
